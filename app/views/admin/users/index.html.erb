<div data-controller="filter">
  <filter-input aria-owns="usersFilterList" class='flex justify-apart'>
    <input placeholder="Filter Users" type="text" />
  </filter-input>
  <div id="usersFilterList">
  <ul data-filter-list>
    <%- @users.each do |user| %>
      <li>
        <span><%= user.name %></span>
        <span><%= link_to(user.name, edit_user_path(user)) %></span>
        <span><%= link_to('Posts', admin_user_posts_path(user)) %></span>
      </li>
    <%- end %>
  </ul>
</div>
